{
    "name": "generator-utilities",
    "packageManager": "yarn@3.5.1",
    "version": "0.2.1",
    "description": "A utility library for generating and processing sequences based on ES6 generators.",
    "main": "index",
    "scripts": {
        "postinstall": "husky install",
        "prepack": "pinst --disable",
        "postpack": "pinst --enable",
        "test": "jest",
        "docs": "jsdoc -c jsdoc.conf.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lachlanhunt/generator-utilities.git"
    },
    "keywords": [
        "generator",
        "ES6"
    ],
    "author": "Lachlan Hunt <lachlan.hunt@lachy.id.au>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/lachlanhunt/generator-utilities/issues"
    },
    "homepage": "https://github.com/lachlanhunt/generator-utilities#readme",
    "dependencies": {
        "arrayslice": "^0.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.21.5",
        "@babel/preset-env": "^7.21.5",
        "@babel/preset-typescript": "^7.21.5",
        "@types/jest": "^29.5.1",
        "@types/node": "^18.16.3",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "@typescript-eslint/parser": "^5.59.1",
        "babel-jest": "^29.5.0",
        "eslint": "^8.39.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "lint-staged": "^13.2.2",
        "pinst": "^3.0.0",
        "prettier": "2.8.8",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    }
}
