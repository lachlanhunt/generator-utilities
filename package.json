{
    "name": "generator-utilities",
    "packageManager": "yarn@4.5.3",
    "version": "0.2.1",
    "description": "A utility library for generating and processing sequences based on ES6 generators.",
    "main": "index",
    "type": "module",
    "scripts": {
        "postinstall": "husky install",
        "prepack": "pinst --disable",
        "postpack": "pinst --enable",
        "test": "vitest",
        "lint": "eslint .",
        "docs": "jsdoc -c jsdoc.conf.json",
        "typecheck": "tsc --noEmit",
        "build": "tsc --project tsconfig.build.json",
        "sdks": "yarn dlx @yarnpkg/sdks vscode"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lachlanhunt/generator-utilities.git"
    },
    "keywords": [
        "generator",
        "ES6"
    ],
    "author": "Lachlan Hunt <lachlan.hunt@lachy.id.au>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/lachlanhunt/generator-utilities/issues"
    },
    "homepage": "https://github.com/lachlanhunt/generator-utilities#readme",
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/preset-env": "^7.26.0",
        "@babel/preset-typescript": "^7.26.0",
        "@eslint/js": "^9.17.0",
        "@types/node": "^22.10.2",
        "@typescript-eslint/parser": "^8.18.1",
        "@vitest/runner": "^2.1.8",
        "@vitest/snapshot": "^2.1.8",
        "eslint": "^9.17.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.7.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "globals": "^15.13.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.2.11",
        "pinst": "^3.0.0",
        "prettier": "3.4.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2",
        "typescript-eslint": "^8.18.1",
        "vitest": "^2.1.8"
    },
    "lint-staged": {
        "**/*": "prettier --write --ignore-unknown"
    }
}
